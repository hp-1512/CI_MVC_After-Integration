<style type="text/css">
    #applyNowBtn{
        width:15rem;
        background-color: transparent;
        border: 2px solid #F88634;
        color: #F88634;
        border-radius: 24px;
    } 
    #applyNowBtn:hover{
        color: #ffff;
        background-color: #F88634;
    }
    .card-tab{
        width:24%;
        height:auto;
    }
    #missionPeriod{
        
        background: white;
        border-radius:20px;
    }
     #mission-details-left{
        padding:0 5%;
    }
    .img-gallary{
        padding-left:5%;
    }
    h3{
        color:#414141;
        font-weight:400;
    }

    .nav-tabs .nav-item.show .nav-link, .nav-tabs .nav-link.active {
        color: #000000;
        background-color: transparent;
        border-color: transparent transparent #f3f3f3;
        border-bottom: 2px solid !important;
        font-size: 17px;
        font-weight:600;
        
    }

    .nav-tabs .nav-link {
        border: 1px solid transparent;
        border-top-left-radius: .25rem;
        border-top-right-radius: .25rem;
        color: #414141;
        font-size: 18px;
    }
    #post-comment-btn {
    background-color: transparent;
    border: 2px solid #F88634;
    color: #F88634;
    border-radius: 24px;
    font-size: 17px;
}

#post-comment-btn:hover{
    color: #ffff;
    background-color: #F88634;
}
.volunteer-img{
    height:70px;
    width:70px;
    border-radius:50%;
}

/*pagination Part*/
.info span span {
    align-items: end;
    font-size: 10px;
}

.info button:first-child {
    border: 0px;
    border-right: 1px solid #E8E8E8;
    background-color: transparent;
    height: 100%;
    width: 10%;
}

.info button:last-child {
    border: 0;
    border-left: 1px solid #E8E8E8;
    background-color: transparent;
    height: 100%;
    width: 10%;
}

.info .pagging {
    height: 50px;
    border: #E8E8E8 1px solid;
}

    @@media only screen and (max-width:579px){
        #mission-info{
            flex-direction:column;
        }
        #mission-info>div{
            text-align:center;
            width:100%;
        }
    }
   
    @@media only screen and (min-width:991px){
        .img-gallary{
            height:38rem;
            
        }
        @@media only screen and (max-width:1140px){
        #add-to-fav, #recommen-to-co{
            height:60px;
        }
        
         
        #mission-info{
            flex-wrap:wrap;
        }
        .card-tab{
            width:48%;
            margin:1%;
        }
        
    }}
    @@media only screen and (max-width:992px){
        .card-body{
            text-align:center;
        }
        .img-gallary{
            padding-left:0;
        }
    }
   
    
